<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="match_parent"
  android:layout_height="wrap_content"
  android:orientation="vertical">
  <SeekBar
    android:id="@+id/vehicle_seek_baar"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="5dip"
    android:progressDrawable="@drawable/vehicle_seekbar"
    android:thumb="@drawable/selected_vehicle"/>
  <RelativeLayout
    android:id="@+id/vehicles_placeholder"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">
    <com.porter.utils.PorterTextView
      android:layout_width="wrap_content"
      android:text="Tata 407"
      style="@style/label_blue_small"
      android:layout_height="wrap_content"/>

    <com.porter.utils.PorterTextView
      android:layout_width="wrap_content"
      android:text="Tata Ace"
      android:layout_alignParentRight="true"
      style="@style/label_blue_small"
      android:layout_height="wrap_content"/>
    </RelativeLayout>
</RelativeLayout>